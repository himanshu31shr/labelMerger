{
  "name": "material-ui-vite-ts",
  "private": true,
  "version": "5.0.0",
  "type": "module",
  "homepage": "https://himanshu31shr.github.io/flipkart-amazon-tools/",
  "scripts": {
    "dev": "concurrently --kill-others --names \"FIREBASE,SEED,VITE\" --prefix-colors \"bgBlue.bold,bgYellow.bold,bgGreen.bold\" \"npm run emulator:start\" \"npm run seed:wait-and-run\" \"npm run dev:wait-and-start\"",
    "dev:wait-and-start": "node -e \"setTimeout(() => process.exit(0), 5000)\" && cross-env VITE_FIREBASE_FIRESTORE_EMULATOR_HOST=localhost VITE_FIREBASE_FIRESTORE_EMULATOR_PORT=8080 VITE_FIREBASE_AUTH_EMULATOR_HOST=localhost VITE_FIREBASE_AUTH_EMULATOR_PORT=9099 vite",
    "dev:vite-only": "vite",
    "dev:emulator": "cross-env VITE_FIREBASE_FIRESTORE_EMULATOR_HOST=localhost VITE_FIREBASE_FIRESTORE_EMULATOR_PORT=8080 VITE_FIREBASE_AUTH_EMULATOR_HOST=localhost VITE_FIREBASE_AUTH_EMULATOR_PORT=9099 vite",
    "emulator:start": "firebase emulators:start --only auth,firestore",
    "emulator:ui": "firebase emulators:start --only auth,firestore --inspect-functions",
    "emulator:export": "firebase emulators:export ./firebase-data",
    "emulator:import": "firebase emulators:start --only auth,firestore --import ./firebase-data",
    "emulator:reset": "firebase emulators:exec --only auth,firestore \"npm run seed:emulator\"",
    "seed:emulator": "node scripts/seed-emulator.js",
    "seed:enhanced": "node scripts/seed-emulator.js",
    "seed:wait-and-run": "node -e \"setTimeout(() => process.exit(0), 4000)\" && npm run seed:emulator",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src/**/*.ts src/**/*.tsx --fix",
    "type-check": "tsc --noEmit",
    "lint-full": "npm run type-check && npm run lint",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:watch:coverage": "npm run test:coverage -- --watch",
    "test:ci": "npm run test -- --watchAll=false",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "npm run test:ci -- --findRelatedTests --passWithNoTests"
    ]
  },
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "latest",
    "@mui/x-date-pickers": "^8.4.0",
    "@reduxjs/toolkit": "^2.8.1",
    "@types/recharts": "^2.0.1",
    "date-fns": "^4.1.0",
    "firebase": "^11.6.1",
    "gh-pages": "^6.2.0",
    "html2pdf.js": "^0.10.3",
    "papaparse": "^5.4.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^2.16.105",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.56.4",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "recharts": "^2.15.3",
    "redux-persist": "^6.0.0",
    "vite-plugin-pwa": "^0.21.1",
    "xlsx": "^0.18.5",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/js": "^9.17.0",
    "@jest/types": "^30.0.0-beta.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/node-fetch": "^2.6.12",
    "@types/papaparse": "^5.3.15",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.0.2",
    "@types/react-redux": "^7.1.34",
    "@types/testing-library__user-event": "^4.2.0",
    "@vitejs/plugin-react": "latest",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "canvas": "^2.11.2",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.0.0",
    "msw": "^2.8.4",
    "node-fetch": "^2.7.0",
    "sharp": "^0.34.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.3.2",
    "whatwg-fetch": "^3.6.20"
  }
}
